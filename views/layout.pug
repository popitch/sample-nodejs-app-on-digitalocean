doctype html

html
    head
        title= title
        link(rel='stylesheet', href='/css/common.css')
        
    body.site-layout
        ul.top-status-line
            li
                | Обновление:
                div.value #{aggregator.process().now}
                | Обменников:
                div.value #{aggregator.exchangersCount()}
                | Всего курсов:
                div.value #{aggregator.pairsCount()}
        
        block content
